#!/bin/bash
#
# Author: Jim Whiteman <jimtron9000@gmail.com>
#
# Print all git objects. Poorly.
#

set -e

# plumbing way to get this?
OBJDIRS=$(ls .git/objects | grep -v info | grep -v pack)

for d in $OBJDIRS
do
  OBJS=$(ls .git/objects/${d})

  for o in $OBJS
  do
    echo "  ${d}${o}:"
    echo $(git cat-file -p ${d}${o})
    echo
  done
done
